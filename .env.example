# This is an example of your .env file format, which pnpm db:setup will create.
# Note: this must be .env, not .env.local, without further configuration changes.
POSTGRES_URL=postgresql://***
STRIPE_SECRET_KEY=sk_test_***
STRIPE_WEBHOOK_SECRET=whsec_***
BASE_URL=http://localhost:3000
AUTH_SECRET=***




# Database
DATABASE_URL=postgresql://user:password@localhost:5432/dbname

# Solana Configuration
RPC_URL=https://api.mainnet-beta.solana.com
GATEWAY_API_KEY=your_sanctum_gateway_api_key
GATEWAY_URL=https://tpg.sanctum.so/v1/mainnet

# Backend Authority (Base58 encoded keypair)
# This wallet will be the delegate authority for all subscriptions
BACKEND_KEYPAIR=your_base58_encoded_keypair
BACKEND_AUTHORITY=your_backend_wallet_public_key
PLATFORM_FEE_WALLET=your_platform_fee_wallet_address

# Cron Secret (for securing the scheduler endpoint)
CRON_SECRET=your_secure_random_string

# Webhook Configuration
WEBHOOK_SECRET=your_webhook_signing_secret

# Next.js
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Optional: Priority Fee Services
HELIUS_API_KEY=your_helius_api_key
TRITON_API_KEY=your_triton_api_key